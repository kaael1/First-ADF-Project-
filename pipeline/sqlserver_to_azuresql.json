{
	"name": "sqlserver_to_azuresql",
	"properties": {
		"activities": [
			{
				"name": "sqlServer_to_azureSql",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "id",
									"type": "Int32",
									"physicalType": "int"
								},
								"sink": {
									"name": "id",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "funcao",
									"type": "String",
									"physicalType": "varchar"
								},
								"sink": {
									"name": "funcao",
									"physicalType": "nvarchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_sqlserver_local",
						"type": "DatasetReference",
						"parameters": {
							"schemaName": "dbo",
							"tableName": "funcao"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_generico_azuresql",
						"type": "DatasetReference",
						"parameters": {
							"schemaName": "SalesLT",
							"tableName": "FuncaoTB"
						}
					}
				]
			}
		],
		"folder": {
			"name": "adf_04_handson_02"
		},
		"annotations": []
	}
}